#include <stdio.h>
#include <stdlib.h>
#include <ctype.h>

/**
 * main - Program entry point
 * @argc: argument count
 * @argv: argument vector
 *
 * Return: 0 - success, any other int otherwise
 */
int main(int argc, char *argv[])
{
	int num, i, result = 0;

	if (argc >= 3)
	{
		for (i = 1; i < argc; i++)
		{
			num = atoi(argv[i]);
			printf("argv[%d] = %d\n,i, num");
			result = result + num;
		}

		printf("%d\n", result);
	}
	else if (argc == 1)
	{
		printf("0\n");
	}
	return (0);
}
